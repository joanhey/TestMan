{
    "name": "joanhey/testman",
    "type": "library",
    "keywords": [
        "tests",
        "php",
        "servers"
    ],
    "license": "MIT",
    "description": "Test any http server.",
    "authors": [
        {
            "name": "Joanhey",
            "email": "joanhey@kumbiaphp.com",
            "homepage": "https://kumbiaphp.com",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/joanhey/testman/issues",
        "source": "https://github.com/joanhey/testman"
    },
    "require": {
        "php": "^8"
    },
    "require-dev": {
        "ext-curl": "*",
        "pestphp/pest": "^2.8",
        "mockery/mockery": "2.0.x-dev",
        "guzzlehttp/guzzle": "^7.0"
    },
    "bin": ["adapterman"],
    "suggest": {
        "ext-event": "For better performance. "
    },
    "autoload-dev": {
        "psr-4": {
            "TestsMam\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
